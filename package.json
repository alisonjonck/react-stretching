{
  "name": "react-stretching",
  "version": "1.0.0",
  "description": "react stretching",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha -r babel-plugin-require-context-hook/register -r @babel/register -r ./src/tests/setup/index.js --recursive \"./src/tests/**/*.test.js*\" --preset babel",
    "test-debug": "mocha --inspect-brk --require babel-register -r ./src/tests/setup/index.js \"./src/tests/**/*.test.js*\"",
    "start": "webpack-dev-server --mode development",
    "lint": "eslint \"src/**\"",
    "build": "webpack",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alisonjonck/react-stretching.git"
  },
  "author": "Alison Henrique Jonck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alisonjonck/react-stretching/issues"
  },
  "homepage": "https://github.com/alisonjonck/react-stretching#readme",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/tests/setup/index.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-airbnb": "^5.0.0",
    "chai": "^4.1.2",
    "codecov": "^3.6.5",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "sinon": "^6.1.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.1.0"
  }
}
